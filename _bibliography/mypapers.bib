 bibtex_show = {true},
 doi = {10.3847/1538-4357/ab9b87},
 eid = {35},
 eprint = {2006.05614},
 journal = {Astrophysical Journal},
 keywords = {Interstellar medium, Magnetohydrodynamical simulations, Disk galaxies, AGN host galaxies, Star formation, Spiral galaxies, Galaxy magnetic fields, Superbubbles, 847, 1966, 391, 2017, 1569, 1560, 604, 1656, Astrophysics - Astrophysics of Galaxies},
 month = {July},
