 journal = {Astrophysical Journal},
 keywords = {galaxies: ISM, galaxies: kinematics and dynamics, galaxies: magnetic fields, galaxies: star formation, magnetohydrodynamics: MHD, turbulence, Astrophysics - Astrophysics of Galaxies},
 month = {December},
