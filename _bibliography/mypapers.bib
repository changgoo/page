 bibtex_show = {true},
 doi = {10.3847/2041-8213/abc252},
 eid = {L34},
 eprint = {2010.09090},
 journal = {Astrophysical Journal, Letters},
 keywords = {Galactic winds, Galaxy winds, Galaxy fountains, Galaxy formation, Stellar feedback, 572, 626, 596, 595, 1602, Astrophysics - Astrophysics of Galaxies},
