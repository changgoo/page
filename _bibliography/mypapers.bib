 keywords = {Stellar winds, Star formation, Chemical enrichment, Stellar wind bubbles, Molecular clouds, Young star clusters, Hydrodynamical simulations, Star clusters, 1636, 1569, 225, 1635, 1072, 1833, 767, 1567, Astrophysics - Astrophysics of Galaxies},
 month = {November},
 number = {1},
