 eid = {18},
 eprint = {2405.02396},
 journal = {Astrophysical Journal},
 keywords = {Stellar winds, Interstellar medium, Star forming regions, Stellar feedback, Stellar wind bubbles, Young star clusters, 1636, 847, 1565, 1602, 1635, 1833, Astrophysics - Astrophysics of Galaxies},
