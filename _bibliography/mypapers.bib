 bibtex_show = {true},
 doi = {10.1093/mnras/stz2773},
 eprint = {1902.09547},
 journal = {Monthly Notices of the Royal Astronomical Society},
 keywords = {ISM: bubbles, ISM: kinematics and dynamics, ISM: supernova remnants, Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
 month = {December},
 number = {2},
 pages = {1961-1990},
 primaryclass = {astro-ph.GA},
 title = {{Evolution of supernovae-driven superbubbles with conduction and cooling}},
