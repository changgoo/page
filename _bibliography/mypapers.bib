 eid = {1},
 eprint = {1308.3231},
 journal = {Astrophysical Journal},
 keywords = {galaxies: ISM, galaxies: kinematics and dynamics, galaxies: star formation, methods: numerical, turbulence, Astrophysics - Astrophysics of Galaxies},
 month = {October},
 number = {1},
 pages = {1},
 primaryclass = {astro-ph.GA},
 title = {{Three-dimensional Hydrodynamic Simulations of Multiphase Galactic Disks with Star Formation Feedback. I. Regulation of Star Formation Rates}},
 volume = {776},
 year = {2013}
}

@article{2011ApJ...743...25K,
 abbr = {ApJ},
 abstract = {{We use vertically resolved numerical hydrodynamic simulations to study
star formation and the interstellar medium (ISM) in galactic
disks. We focus on outer-disk regions where diffuse H I
dominates, with gas surface densities {\ensuremath{\Sigma}} =
3-20 M$_{{\ensuremath{\odot}}}$ pc$^{-2}$ and star-plus-dark
matter volume densities {\ensuremath{\rho}}$_{sd}$ = 0.003-0.5
M$_{{\ensuremath{\odot}}}$ pc$^{-3}$. Star formation occurs in
very dense, self-gravitating clouds that form by mergers of
smaller cold cloudlets. Turbulence, driven by momentum feedback
from supernova events, destroys bound clouds and puffs up the
disk vertically. Time-dependent radiative heating (FUV from
recent star formation) offsets gas cooling. We use our
simulations to test a new theory for self-regulated star
formation. Consistent with this theory, the disks evolve to a
state of vertical dynamical equilibrium and thermal equilibrium
with both warm and cold phases. The range of star formation
surface densities and midplane thermal pressures is
{\ensuremath{\Sigma}}$_{SFR}$ {\ensuremath{\sim}} 10$^{-4}$ to
10$^{-2}$ M$_{{\ensuremath{\odot}}}$ kpc$^{-2}$ yr$^{-1}$ and P
$_{th}$/k $_{B}$ {\ensuremath{\sim}} {}10$^{2}$ to {}10$^{4}$
cm$^{-3}$ K. In agreement with observations, turbulent velocity
dispersions are \raisebox{-0.5ex}\textasciitilde7 km s$^{-1}$
and the ratio of the total (effective) to thermal pressure is
P$_{tot}$/P$_{th}$ {\ensuremath{\sim}} 4-5, across this whole
range (provided shielding is similar to the solar neighborhood).
We show that {\ensuremath{\Sigma}}$_{SFR}$ is not well
correlated with {\ensuremath{\Sigma}} alone, but rather with
{\ensuremath{\Sigma}} {\ensuremath{\rho}}$_{sd}$$^{1/2}$,
because the vertical gravity from stars and dark matter
dominates in outer disks. We also find that
{\ensuremath{\Sigma}}$_{SFR}$ has a strong, nearly linear
correlation with P$_{tot}$, which itself is within
\raisebox{-0.5ex}\textasciitilde13\% of the dynamical
equilibrium estimate P$_{tot, DE}$. The quantitative
relationships we find between \&Sigma$_{SFR}$ and the turbulent
and thermal pressures show that star formation is highly
efficient for energy and momentum production, in contrast to the
low efficiency of mass consumption. Star formation rates adjust
until the ISM's energy and momentum losses are replenished by
feedback within a dynamical time.}},
 adsnote = {Provided by the SAO/NASA Astrophysics Data System},
 adsurl = {https://ui.adsabs.harvard.edu/abs/2011ApJ...743...25K},
