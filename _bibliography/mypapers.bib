 bibtex_show = {true},
 doi = {10.3847/1538-4357/ad74f0},
 eid = {201},
 eprint = {2404.15396},
 journal = {Astrophysical Journal},
 keywords = {Supernova remnants, Shocks, Hydrodynamics, Cosmic rays, Non-thermal radiation sources, 1667, 2086, 1963, 329, 1119, Astrophysics - High Energy Astrophysical Phenomena},
 month = {October},
 number = {2},
 pages = {201},
