 journal = {Astrophysical Journal},
 keywords = {Cold neutral medium, H II regions, Interstellar absorption, Interstellar atomic gas, Interstellar phases, Interstellar molecules, Interstellar medium, Large Magellanic Cloud, Molecular clouds, Photodissociation regions, Radio astronomy, 266, 694, 831, 833, 850, 849, 847, 903, 1072, 1223, 1338, Astrophysics of Galaxies},
