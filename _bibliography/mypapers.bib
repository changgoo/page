 author = {{Linzer}, Nora B. and {Kim}, Jeong-Gyu and {Kim}, Chang-Goo and {Ostriker}, Eve C.},
 bibtex_show = {true},
 doi = {10.3847/1538-4357/ad7733},
 eid = {173},
 eprint = {2409.05958},
 journal = {Astrophysical Journal},
 keywords = {Interstellar medium, Interstellar radiation field, Magnetohydrodynamical simulations, 847, 852, 1966, Astrophysics - Astrophysics of Galaxies},
 month = {November},
 number = {2},
 pages = {173},
 primaryclass = {astro-ph.GA},
