 bibtex_show = {true},
 doi = {10.3847/1538-4357/acc250},
 eid = {114},
 eprint = {2303.04206},
 journal = {Astrophysical Journal},
 keywords = {Star formation, Galaxy circumnuclear disk, Barred spiral galaxies, Stellar feedback, Interstellar medium, Magnetohydrodynamics, Magnetohydrodynamical simulations, Galaxy nuclei, 1569, 581, 136, 1602, 847, 1964, 1966, 609, Astrophysics - Astrophysics of Galaxies},
 month = {April},
