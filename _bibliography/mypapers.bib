@article{2024ApJ...973..141G,
 abbr = {ApJ},
 abstract = {{We present 3D magnetohydrodynamic simulations of the fueling of
supermassive black holes in elliptical galaxies from a turbulent
cooling medium on galactic scales, taking M87* as a typical
case. We find that the mass accretion rate is increased by a
factor of {\ensuremath{\sim}}10 compared with analogous
hydrodynamic simulations. The scaling of
Ṁ{\ensuremath{\sim}}r1/2 roughly holds from
{\ensuremath{\sim}}10 pc to {\ensuremath{\sim}}10$^{‑3}$ pc
({\ensuremath{\sim}}10 r $_{g}$) with the accretion rate through
the event horizon being {\ensuremath{\sim}}10$^{‑2}$ M
$_{{\ensuremath{\odot}}}$ yr$^{‑1}$. The accretion flow on
scales {\ensuremath{\sim}}0.03{\textendash}3 kpc takes the form
of magnetized filaments. Within {\ensuremath{\sim}}30 pc, the
cold gas circularizes, forming a highly magnetized
({\ensuremath{\beta}} {\ensuremath{\sim}} 10$^{‑3}$) thick disk
supported by a primarily toroidal magnetic field. The cold disk
is truncated and transitions to a turbulent hot accretion flow
at {\ensuremath{\sim}}0.3 pc ({}10$^{3}$ r $_{g}$). There are
strong outflows toward the poles driven by the magnetic field.
The outflow energy flux increases with smaller accretor size,
reaching {\ensuremath{\sim}}3 {\texttimes} {}10$^{43}$ erg
s$^{‑1}$ for r $_{in}$ = 8 r $_{g}$; this corresponds to a
nearly constant energy feedback efficiency of
{\ensuremath{\eta}} {\ensuremath{\sim}} 0.05{\textendash}0.1
independent of accretor size. The feedback energy is enough to
balance the total cooling of the M87/Virgo hot halo out to
{\ensuremath{\sim}}50 kpc. The accreted magnetic flux at small
radii is similar to that in magnetically arrested disk models,
consistent with the formation of a powerful jet on horizon
scales in M87. Our results motivate a subgrid model for
accretion in lower-resolution simulations in which the hot gas
accretion rate is suppressed relative to the Bondi rate by
{\ensuremath{\sim}}(10rg/rB)1/2.}},
 adsnote = {Provided by the SAO/NASA Astrophysics Data System},
