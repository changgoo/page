 month = {August},
 number = {1},
 pages = {43},
 primaryclass = {astro-ph.GA},
 title = {{The Coevolution of Stellar Wind-blown Bubbles and Photoionized Gas. II. 3D RMHD Simulations and Tests of Semianalytic Models}},
