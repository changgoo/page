 journal = {Astrophysical Journal},
 keywords = {Computational methods, Interstellar dust, Interstellar magnetic fields, Interstellar medium, Magnetohydrodynamical simulations, Polarimetry, Astrophysics - Astrophysics of Galaxies, Astrophysics - Cosmology and Nongalactic Astrophysics},
 month = {August},
 number = {2},
