 keywords = {Astrochemistry, Magnetohydrodynamical simulations, Interstellar medium, 75, 1966, 847, Astrophysics - Astrophysics of Galaxies, Astrophysics - Earth and Planetary Astrophysics, Astrophysics - Instrumentation and Methods for Astrophysics, Astrophysics - Solar and Stellar Astrophysics},
