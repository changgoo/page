 author = {{Kado-Fong}, Erin and {Kim}, Jeong-Gyu and {Ostriker}, Eve C. and {Kim}, Chang-Goo},
 bibtex_show = {true},
 doi = {10.3847/1538-4357/ab9abd},
 eid = {143},
 eprint = {2006.06697},
 journal = {Astrophysical Journal},
 keywords = {Interstellar medium, Warm ionized medium, Galaxy structure, Radiative transfer, 1335, 1788, 847, 622, Astrophysics - Astrophysics of Galaxies},
