 author = {{Smith}, Matthew C. and {Fielding}, Drummond B. and {Bryan}, Greg L. and {Kim}, Chang-Goo and {Ostriker}, Eve C. and {Somerville}, Rachel S. and {Stern}, Jonathan and {Su}, Kung-Yi and {Weinberger}, Rainer and {Hu}, Chia-Yu and {Forbes}, John C. and {Hernquist}, Lars and {Burkhart}, Blakesley and {Li}, Yuan},
 bibtex_show = {true},
 doi = {10.1093/mnras/stad3168},
 eprint = {2301.07116},
 journal = {Monthly Notices of the Royal Astronomical Society},
 keywords = {hydrodynamics, methods: numerical, galaxies: evolution, Astrophysics - Astrophysics of Galaxies},
 month = {January},
 number = {1},
 pages = {1216-1243},
 primaryclass = {astro-ph.GA},
 title = {{ARKENSTONE - I. A novel method for robustly capturing high specific energy outflows in cosmological simulations}},
 volume = {527},
 year = {2024}
}

@article{2024ApJ...960..100S,
 abbr = {ApJ},
 abstract = {{We present the first results from a high-resolution simulation with a
focus on galactic wind driving for an isolated galaxy with a
halo mass of \raisebox{-0.5ex}\textasciitilde{}10$^{11}$ M
$_{{\ensuremath{\odot}}}$ (similar to the Large Magellanic
Cloud) and a total gas mass of \raisebox{-0.5ex}\textasciitilde6
