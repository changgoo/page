 author = {{Vijayan}, Aditi and {Kim}, Chang-Goo and {Armillotta}, Lucia and {Ostriker}, Eve C. and {Li}, Miao},
 bibtex_show = {true},
 doi = {10.3847/1538-4357/ab8474},
 eid = {12},
 eprint = {1911.07872},
 journal = {Astrophysical Journal},
 keywords = {Magnetohydrodynamical simulations, Interstellar medium, Galaxy fountains, Galaxy winds, Stellar feedback, 1966, 847, 596, 626, 1602, Astrophysics - Astrophysics of Galaxies},
 month = {May},
 number = {1},
