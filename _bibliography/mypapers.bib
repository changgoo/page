 bibtex_show = {true},
 doi = {10.3847/1538-4357/abbdab},
 eid = {142},
 eprint = {2009.14631},
 journal = {Astrophysical Journal},
 keywords = {Interstellar medium, Interstellar molecules, Molecular clouds, Galaxy disks, Magnetohydrodynamical simulations, Chemical abundances, 847, 849, 1072, 589, 1966, 224, Astrophysics - Astrophysics of Galaxies, Astrophysics - Solar and Stellar Astrophysics},
 month = {November},
