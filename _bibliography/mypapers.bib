 journal = {Astrophysical Journal},
 keywords = {galaxies: ISM, galaxies: star formation, magnetohydrodynamics: MHD, methods: numerical, Astrophysics - Astrophysics of Galaxies},
 month = {February},
 number = {2},
 pages = {173},
 primaryclass = {astro-ph.GA},
 title = {{Numerical Simulations of Multiphase Winds and Fountains from Star-forming Galactic Disks. I. Solar Neighborhood TIGRESS Model}},
